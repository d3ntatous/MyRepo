<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/UI.xsd">
  <Button name="FrameColorOptions_ModuleColorButtonTemplate" inherits="FrameColorOptions_ColorButtonTemplate" virtual="true">
    <Layers>
      <Layer level="OVERLAY">
        <FontString parentKey="name" inherits="GameFontNormalLeft">
          <Anchors>
            <Anchor point="LEFT" x="50" y="0"/>
          </Anchors>
        </FontString>
      </Layer>
		</Layers>
    <Frames>
      <CheckButton parentKey="lockColorButton">
        <Size x="24" y="30"/>
        <Anchors>
          <Anchor point="LEFT" x="10"/>
        </Anchors>
        <HighlightTexture atlas="Professions_Specialization_Lock_Glow"/>
        <NormalTexture atlas="Azerite-Trait-Padlock"/>
        <PushedTexture atlas="UI-CharacterCreate-PadLock"/>
        <CheckedTexture atlas="UI-CharacterCreate-PadLock"/>
      </CheckButton>
		</Frames>
  </Button>
</Ui>
