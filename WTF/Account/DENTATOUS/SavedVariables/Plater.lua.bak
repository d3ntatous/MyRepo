ame = \"Debuff ID #4\",\n            desc = \"Enter the spell ID of the fourth debuff to track\",\n            value = \"\",\n        },\n        {\n            type = \"color\",\n            key = \"color4\",\n            name = \"Color #4\",\n            desc = \"Choose the color for debuff #4\",\n            value = {1, 1, 0, 1},\n        },\n        -- Debuff-Color Pair 5\n        {\n            type = \"text\",\n            key = \"debuff5\",\n            name = \"Debuff ID #5\",\n            desc = \"Enter the spell ID of the fifth debuff to track\",\n            value = \"\",\n        },\n        {\n            type = \"color\",\n            key = \"color5\",\n            name = \"Color #5\",\n            desc = \"Choose the color for debuff #5\",\n            value = {1, 0, 1, 1},\n        },\n        -- Debuff-Color Pair 6\n        {\n            type = \"text\",\n            key = \"debuff6\",\n            name = \"Debuff ID #6\",\n            desc = \"Enter the spell ID of the sixth debuff to track\",\n            value = \"\",\n        },\n        {\n            type = \"color\",\n            key = \"color6\",\n            name = \"Color #6\",\n            desc = \"Choose the color for debuff #6\",\n            value = {0, 1, 1, 1},\n        },\n        -- Debuff-Color Pair 7\n        {\n            type = \"text\",\n            key = \"debuff7\",\n            name = \"Debuff ID #7\",\n            desc = \"Enter the spell ID of the seventh debuff to track\",\n            value = \"\",\n        },\n        {\n            type = \"color\",\n            key = \"color7\",\n            name = \"Color #7\",\n            desc = \"Choose the color for debuff #7\",\n            value = {0.5, 0, 1, 1},\n        },\n        -- Debuff-Color Pair 8\n        {\n            type = \"text\",\n            key = \"debuff8\",\n            name = \"Debuff ID #8\",\n            desc = \"Enter the spell ID of the eighth debuff to track\",\n            value = \"\",\n        },\n        {\n            type = \"color\",\n            key = \"color8\",\n            name = \"Color #8\",\n            desc = \"Choose the color for debuff #8\",\n            value = {1, 0.5, 0, 1},\n        },\n        -- Debuff-Color Pair 9\n        {\n            type = \"text\",\n            key = \"debuff9\",\n            name = \"Debuff ID #9\",\n            desc = \"Enter the spell ID of the ninth debuff to track\",\n            value = \"\",\n        },\n        {\n            type = \"color\",\n            key = \"color9\",\n            name = \"Color #9\",\n            desc = \"Choose the color for debuff #9\",\n            value = {0, 0.5, 1, 1},\n        }\n    }\nend",
