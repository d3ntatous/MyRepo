["names"] = {
},
["custom"] = "function(allstates, event, ...)\n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        --idea is to announce amount of barrel stacks that you wll get via windows TTS\n        local timestamp, subEvent, hideCaster, sourceGUID, sourceName, sourceFlags, sourceRaidFlags, destGUID, destName, destFlags, destRaidFlags, spellID, _,_, auraType = ...\n        if subEvent == \"SPELL_CAST_SUCCESS\"\n        and spellID == 435797 then\n            local npcID = select(6, strsplit(\"-\", sourceGUID))\n            if npcID == \"224911\" then\n                local state = allstates[\"\"]\n                if state then\n                    state.stacks = state.stacks + 1\n                    state.changed = true\n                    return true\n                else\n                    C_Timer.After(0.2, function() WeakAuras.ScanEvents(\"CAUSESE_BARREL\", \"nilcheck\") end)\n                    allstates[\"\"] = {\n                        show = true,\n                        changed = true,\n                        progressType = \"timed\",\n                        duration = 3,\n                        expirationTime = 3 + GetTime(),\n                        stacks  = 1,\n                        autoHide = true,\n                    }\n                    return true\n                end\n            end\n        end\n    elseif event == \"CAUSESE_BARREL\" and ... then\n        local state = allstates[\"\"]\n        if state then\n            state.finalStacks = state.stacks\n            state.changed = true\n            return true    \n        end\n    end\nend",
