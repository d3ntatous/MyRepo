["custom"] = "function(allstates, event, ...)\n    if aura_env.loaded then\n        if event == \"UNIT_SPELLCAST_START\"\n        or (event == \"UNIT_SPELLCAST_CHANNEL_START\" and aura_env.config.channel)\n        or event == \"NAME_PLATE_UNIT_ADDED\" then\n            local unit = ...\n            local name, duration, expirationTime, icon, notInterruptible, spellID, castType = CauseseAPI:GetCastInfo(unit)\n            if spellID then\n                local spell = aura_env.spellIDs[spellID] or CauseseDB.tank[spellID]\n                if spell\n                --and UnitIsUnit(unit..\"target\", \"player\") \n                then\n                    local customName, categoryNumber, show, customSound = unpack(spell)\n                    if spellID and show then \n                        local guid = UnitGUID(unit)\n                        if not aura_env.config.disableSounds and customSound and customSound ~= \"\" and castType == \"cast\" then\n                            if aura_env.config.singleSound then\n                                local duplicate = false\n                                for _, state in pairs(allstates) do\n                                    if state.spellID == spellID then\n                                        duplicate = true\n                                        break\n                                    end\n                                end\n                                if not duplicate then\n                                    PlaySoundFile(\"Interface\\\\AddOns\\\\SharedMedia_Causese\\\\sound\\\\\"..customSound, \"MASTER\")\n                                end\n                            else\n                                PlaySoundFile(\"Interface\\\\AddOns\\\\SharedMedia_Causese\\\\sound\\\\\"..customSound, \"MASTER\")\n                            end\n                        end\n                        allstates[guid] = {\n                            show = true,\n                            changed = true,\n                            progressType = \"timed\",\n                            duration = duration,\n                            expirationTime = expirationTime,\n                            castType = castType,\n                            spell = customName or \"\",\n                            spellName = name,\n                            icon = icon,\n                            spellID = spellID,\n                            categoryNumber = categoryNumber or -1,\n                            autoHide = true,\n                        } \n                        return true\n                    end\n                end\n            end\n            \n        elseif event == \"UNIT_SPELLCAST_STOP\"\n        or event == \"UNIT_SPELLCAST_CHANNEL_STOP\"\n        or event == \"NAME_PLATE_UNIT_REMOVED\" then\n            local unit = ...\n            local state = allstates[UnitGUID(unit)]\n            if state then\n                state.show = false\n                state.changed = true\n                return true\n            end\n        end\n    end\nend",
