["customText"] = "function()\n    --this is for the preview, applied via conditions\n    if WeakAuras.IsOptionsOpen() then \n        local display = aura_env.config.display \n        if display == 1 then\n            return \"Buster\"\n        elseif display == 2 then\n            return \"Tenderize\"\n        end\n        \n    elseif aura_env.state then\n        local display = aura_env.config.display \n        if display == 1 and aura_env.state.spell then\n            return string.format(\"%s\", aura_env.state.spell)\n        elseif display == 2 and aura_env.state.spellName then\n            return string.format(\"%s\", aura_env.state.spellName)\n        end\n    end\nend",
