["Constructor"] = "function (self, unitId, unitFrame, envTable)\n    --Important: lines starting with double dashes are comments and are not part of the script\n    \n    --set this to true if you are not using threat colors in the health bar\n    envTable.ForceRefreshNameplateColor = true\n    \n    --if the unit does not have any of the following auras, it will be painted with the color listed below\n    --list of spells to track, can be the spell name (case-sensitive) or the spellID\n    envTable.TrackingAuras = {\n        [157736]=true, \n        [348]=true, \n        [342938]=true, \n        [164815]=true, \n        [228287]=true, \n        [188389]=true, \n        [217200]=true, \n        [270329]=true, \n        [204197]=true, \n        [204213]=true, \n        [34914]=true,\n        [1943]=true, \n        --[270569]=true, \n        [287280]=true, \n        [383346]=true,\n        [1079]=true,\n        [316099]=true,\n        \n        [55095]= true,\n        [425719]= true,\n        [408665] = true,\n        [228358]= true,\n        [388539]=true,\n        [204598]=true,\n        [445474]=true, -- Wither (Warlock)\n        [388106]=true, \n    }\n    \n    envTable.TrackingAuras2 = {\n        [343721] = true, \n        [343527]=true,\n        [196937]=true,\n        [703]=true, \n        [164812]=true,\n        [155722]=true,\n        [295311]=true,\n        [334168]=true,\n        [80240]=true,\n        [423583]=true,\n        [214621]= true,\n        [335467]=true,\n        [262115]=true,\n        [204021]=true,\n        [207771]=true,\n        [191587] =true,\n        [197209] =true, --Enhanced Cobalt\n        [980] =true,\n    }\n    \n    --which color the nameplate wil be changed\n    --color can be added as:\n    --color names: \"red\", \"yellow\"\n    --color hex: \"#FF0000\", \"#FFFF00\"\n    --color table: {1, 0, 0}, {1, 1, 0}    \n    --you may also use /plater colors\n    --envTable.NameplateColor = modTable.config.dot1\n    -- envTable.NameplateColor2 = modTable.config.dot2\n    -- envTable.NameplateColor3 = modTable.config.dot3\n    \n    \n    \nend",
["Nameplate Updated"] = "function (self, unitId, unitFrame, envTable,modTable)\n    \n    if (unitFrame.namePlateIsTarget) then\n        unitFrame:SetAlpha (1)\n    end\n    --do nothing if the player isn't in combat\n    --if (not Plater.IsInCombat()) then\n    --return \n    --end\n    \n    --do nothing if the unit isn't in combat\n    -- if (not unitFrame.InCombat) then\n    --return\n    -- end\n    \n    --do nothing if the unit is the player it self\n    --    if (unitFrame.IsSelf) then\n    --  return\n    --   end\n    local color = Plater.GetNpcColor(unitFrame)\n    --check the auras\n    local hasAura = false\n    local hasAura2 = false\n    \n    for auraName, _ in pairs (envTable.TrackingAuras) do\n        if (Plater.NameplateHasAura (unitFrame, auraName)) then\n            hasAura = true\n            break\n        end\n    end\n    \n    for auraName, _ in pairs (envTable.TrackingAuras2) do\n        if (Plater.NameplateHasAura (unitFrame, auraName)) then\n            hasAura2 = true\n            break\n        end\n    end\n    \n    -- function modTable.UpdateColor (unitFrame)\n    if (unitFrame.namePlateIsTarget and modTable.config.target ) then \n        Plater.SetNameplateColor (unitFrame, modTable.config.tarcolor)\n        -- elseif  then\n    elseif ( hasAura and hasAura2 and modTable.config.dotcolor ) then\n        Plater.SetNameplateColor (unitFrame, modTable.config.dot3)\n    elseif ( hasAura and modTable.config.dotcolor ) then\n        Plater.SetNameplateColor (unitFrame, modTable.config.dot1)\n    elseif ( hasAura2 and modTable.config.dotcolor) then\n        Plater.SetNameplateColor (unitFrame, modTable.config.dot2)\n        --end\n        --end\n        --Plater.SetNameplateColor (unitFrame) \n        -- local color = \n    elseif color then \n        --local color = Plater.GetNpcColor(unitFrame)\n        --if color then \n        Plater.SetNameplateColor (unitFrame, color)\n        -- Plater.SetNameplateColor (unitFrame, color)\n    else\n        --Plater.SetNameplateColor (unitFrame)\n        Plater.RefreshNameplateColor (unitFrame)\n    end\n    --else\n    --  if (envTable.ForceRefreshNameplateColor) then\n    -- Plater.RefreshNameplateColor (unitFrame) \n    --end\n    --end    \n    --end\nend\n\n\n\n",
},
